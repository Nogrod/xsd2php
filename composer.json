{
  "name": "goetas-webservices/xsd2php",
  "description": "Convert XSD  (XML Schema) definitions into PHP classes and JMS metadata",
  "type": "library",
  "authors": [
    {
      "name": "Asmir Mustafic",
      "email" : "goetas@gmail.com"
    }
  ],
  "keywords": [
    "converter",
    "xml",
    "xsd",
    "php",
    "jms",
    "serializer"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "symfony/console": "^4.0|^5.0",
    "symfony/dependency-injection": "^4.0|^5.0",
    "symfony/yaml": "^4.0|^5.0",
    "symfony/config": "^4.0|^5.0",
    "goetas-webservices/xsd-reader": "^0.2.8|^0.3.5",
    "nogrod/xml-client-runtime": "^0.2",
    "doctrine/inflector": "^2.0",
    "laminas/laminas-code": "^4.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0",
    "symfony/validator": "^4.0|^5.0",
    "ext-xmldiff": "*"
  },
  "autoload": {
    "psr-4": {
      "GoetasWebservices\\Xsd\\XsdToPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev"
    }
  },
  "bin": [
    "bin/xsd2php"
  ],
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
